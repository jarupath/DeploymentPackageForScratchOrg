    <validationRules>
        <fullName>ROE_USD</fullName>
        <active>true</active>
        <description>Validation Rule for ROE</description>
        <errorConditionFormula>AND( 
OR($Profile.Name &lt;&gt; &quot;Ground Customer Service&quot;) 

, 
OR($Profile.Name &lt;&gt; &quot;Ground Customer Service Not BKK&quot;) 

,
OR(RecordType.Name == &quot;SD/SR Compensation&quot;, 
RecordType.Name == &quot;SD/SR Compensation Final&quot;) 
, 
OR(Ispickval(Compensation_Tools__c , &quot;Cash&quot;), 
Ispickval(Compensation_Tools__c , &quot;EMD&quot;)) 
, 
OR( 
NOT(Ispickval(Currency__c , &quot;USD&quot;)), 
ISBLANK(TEXT(Currency__c))) 
, 
OR( ROE_US_Dollars__c  &lt;= 0, ISBLANK( ROE_US_Dollars__c )))</errorConditionFormula>
        <errorDisplayField>ROE_US_Dollars__c</errorDisplayField>
        <errorMessage>ROE (USD) is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ROE_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR($Profile.Name &lt;&gt; &quot;Ground Customer Service&quot;)

,
OR($Profile.Name &lt;&gt; &quot;Ground Customer Service Not BKK&quot;) 

,
OR(RecordType.Name == &quot;CFMS Compensation&quot;,
RecordType.Name == &quot;CFMS Compensation Final&quot;)
,
OR(Ispickval(Compensation_Tools__c , &quot;Cash&quot;),
Ispickval(Compensation_Tools__c , &quot;EMD&quot;))
,
OR(
NOT(Ispickval(Currency__c , &quot;THB&quot;)),
ISBLANK(TEXT(Currency__c)))
,
OR(ROE__c &lt;= 0, ISBLANK(ROE__c)))</errorConditionFormula>
        <errorDisplayField>ROE__c</errorDisplayField>
        <errorMessage>ROE is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
OR(ISPICKVAL(Compensation_Tools__c,&apos;Cash&apos;), ISPICKVAL(Compensation_Tools__c,&apos;EMD&apos;)), 
OR(  Amount__c == 0,ISBLANK(Amount__c)), 
OR($User.Department == &apos;S4&apos;, $User.Department = &apos;S3&apos;, $User.Department = &apos;AO&apos;)
)</errorConditionFormula>
        <errorDisplayField>Amount__c</errorDisplayField>
        <errorMessage>Please identify Amount if compensation tool is &apos;Cash&apos; or &apos;EMD&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Approved_Status_OnCeate</fullName>
        <active>true</active>
        <errorConditionFormula>(  RecordType.DeveloperName == &apos;CFMS_Compensation&apos; || RecordType.DeveloperName == &apos;SD_SR_Compensation&apos;) &amp;&amp; ISNEW() &amp;&amp; ISPICKVAL(Status__c , &apos;Approved&apos; )</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Cannot create new compensation with Approved status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_total</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT(ISPICKVAL(Compensation_Tools__c,&apos;Cash&apos;)), NOT(ISPICKVAL(Compensation_Tools__c,&apos;EMD&apos;)), 
OR( Total__c == 0,ISBLANK(Total__c )), 
OR($User.Department == &apos;S4&apos;, $User.Department = &apos;S3&apos;, $User.Department = &apos;AO&apos;)
)</errorConditionFormula>
        <errorDisplayField>Total__c</errorDisplayField>
        <errorMessage>Please identify Total if compensation tool is not &apos;Cash&apos; or &apos;EMD&apos;.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
    <webLinks>
        <fullName>Confirm</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Confirm</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/26.0/connection.js&quot;)}

if({!ISPICKVAL( Compensation__c.Status__c , &quot;Draft&quot;)}){

     if (confirm(&quot;Are you sure?&quot;) == true) { 
         var updatedCompensation = new sforce.SObject(&apos;Compensation__c&apos;); 
         updatedCompensation.id = &quot;{!Compensation__c.Id}&quot;;
         updatedCompensation.status__c = &quot;New&quot;;
         sforce.connection.update([updatedCompensation]);
         window.location.reload();
     }

}else{alert(&quot;This already confirm!&quot;);}</url>
    </webLinks>

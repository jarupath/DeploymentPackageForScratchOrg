        <externalId>false</externalId>
        <formula>IMAGE(&apos;/resource/1476948910000/BlueProgressBar&apos;, &apos;Test&apos;, 10, if(Progress_Date__c &lt;=4,Progress_Date__c ,4) * 10) &amp;
IF(Progress_Date__c &gt; 4,IMAGE(&apos;/resource/1476952717000/RedProgressBar&apos;, &apos;Test&apos;, 10, Progress_Date__c  * 10 - 40),&quot;&quot;) &amp;
IMAGE(&apos;/resource/1476964875000/ProgressBorder&apos;, &apos;Test&apos;, 10, 
((ActivityDate - DATEVALUE(CreatedDate))*10) - (Progress_Date__c * 10)) &amp;
&apos; &apos; &amp;
TEXT (Progress_Date__c) &amp;
&apos; days&apos;</formula>
